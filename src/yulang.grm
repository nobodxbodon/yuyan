%name YuLang;

%tokens
  : ID of string
  | LEFT_PAREN ("「")
  | RIGHT_PAREN ("」");

file(env) : component@(env)+ => (RawAST.RawList(component));

component(env) : LEFT_PAREN component@(env)+ RIGHT_PAREN => (RawAST.RawList(component))
               | ID => (RawAST.RawID(ID));
